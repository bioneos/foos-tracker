{
  "autoInstall": true,
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../public",
      "uncompressed": "dist/components",
      "compressed": "dist/components/",
      "themes": "../public/themes/"
    },
    "clean": "public/"
  },
  "permission": false,
  "rtl": "No",
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "header",
    "icon",
    "image",
    "input",
    "label",
    "list",
    "loader",
    "segment",
    "form",
    "grid",
    "menu",
    "message",
    "table",
    "accordion",
    "checkbox",
    "dropdown",
    "modal",
    "transition",
    "api",
    "form",
    "state"
  ],
  "version": "2.2.2"
}
